{
  "name": "cinema-backend",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node src/server.ts",
    "db:start": "docker-compose -f docker-compose.db.yml up -d",
    "db:stop": "docker-compose -f docker-compose.db.yml down",
    "db:logs": "docker-compose -f docker-compose.db.yml logs -f",
    "db:reset": "npm run db:stop && npm run db:start",
    "migrate:up": "ts-node src/db/run-migrations.ts up",
    "migrate:down": "ts-node src/db/run-migrations.ts down",
    "migrate:create": "ts-node src/db/create-migration.ts",
    "seed": "ts-node src/db/run-seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0",
    "sequelize": "^6.37.7",
    "umzug": "^3.8.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/pg": "^8.15.2",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
